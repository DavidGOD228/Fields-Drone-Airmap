{
  "name": "drone-app",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "@fortawesome/fontawesome-svg-core": "^1.2.24",
    "@fortawesome/free-solid-svg-icons": "^5.11.1",
    "@fortawesome/react-fontawesome": "^0.1.4",
    "@google/maps": "^1.0.1",
    "autoprefixer": "^9.6.1",
    "eslint": "^5.16.0",
    "eslint-config-airbnb": "^18.0.1",
    "eslint-plugin-import": "^2.18.2",
    "eslint-plugin-jsx-a11y": "^6.2.3",
    "eslint-plugin-react": "^7.14.3",
    "google-map-react": "^1.1.5",
    "google-maps-react": "^2.0.2",
    "postcss-cli": "^6.1.3",
    "react": "^16.9.0",
    "react-dom": "^16.9.0",
    "react-inner-html": "^1.0.1",
    "react-router-dom": "^5.0.1",
    "react-scripts": "0.9.5",
    "tailwindcss": "^1.1.2"
  },
  "devDependencies": {
    "asar": "^2.0.1",
    "electron-builder": "^21.2.0",
    "electron-packager": "^14.0.6",
    "electron": "^6.0.9"
  },
  "homepage": "./",
  "main": "src/start.js",
  "scripts": {
    "build:css": "postcss src/styles/index.css -o src/index.css",
    "watch:css": "postcss src/styles/index.css -o src/index.css -w",
    "start": "nf start -p 3000",
    "test": "react-scripts test --env=jsdom",
    "eject": "react-scripts eject",
    "electron": "electron .",
    "electron-start": "node src/start-react",
    "react-start": "react-scripts start",
    "pack": "build --dir",
    "dist": "npm run build && build",
    "postinstall": "install-app-deps",
    "package": "",
    "build": "electron-packager . Drone"
  },
  "build": {
    "appId": "com.electron.electron-with-create-react-app",
    "win": {
      "iconUrl": "https://cdn2.iconfinder.com/data/icons/designer-skills/128/react-256.png"
    },
    "directories": {
      "buildResources": "public"
    }
  }
}
